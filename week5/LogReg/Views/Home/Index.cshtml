@model LogReg.Models.Users
@{
    ViewData["Title"] = "A Web Page";
}
<h1>Register</h1>
<form asp-action="Register" method="post">
    <div class="form-group">
        <span asp-validation-for="first_name"></span>
        <label asp-for="first_name"></label> 
        <input asp-format="text" asp-for="first_name">
    </div>
    <div class="form-group">
        <span asp-validation-for="last_name"></span>
        <label asp-for="last_name"></label> 
        <input asp-format="text" asp-for="last_name">
    </div>
    <div class="form-group">
        <span asp-validation-for="email"></span>
        <label asp-for="email"></label> 
        <input asp-format="text" asp-for="email">
    </div>
    <div class="form-group">
        <span asp-validation-for="password"></span>
        <label asp-for="password"></label> 
        <input asp-format="password" asp-for="password">
    </div>
    <div class="form-group">
        <span asp-validation-for="cpassword"></span>
        <label asp-for="cpassword"></label> 
        <input asp-format="password" asp-for="cpassword">
    </div>
    <button type="submit">Submit</button>
</form>
<h1>Login</h1>
<form asp-action="Login" method="post">
    <div class="form-group">
        <span asp-validation-for="email"></span>
        <label asp-for="email"></label> 
        <input asp-format="text" asp-for="email">
    </div>
    <div class="form-group">
        <span asp-validation-for="password"></span>
        <label asp-for="password"></label> 
        <input asp-format="password" asp-for="password">
    </div>
    <button type="submit">Submit</button>
</form>